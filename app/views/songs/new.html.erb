<% if @song.errors.any? %>
<div id="error_explanation">
  <% @song.errors.full_messages.each do |msg| %>
  <li><%= msg%></li>
  <%end%>
</ul>
</div>
<%end%>


<%= form_for(@song) do |s| %>
<%= s.text_field :title %>
<%= s.text_field :release_year %>
<%= s.text_field :released %>
<%= s.text_field :genre %>
<%= s.text_field :artist_name %>
<%= s.submit "Create Song" %>



<%end%>
